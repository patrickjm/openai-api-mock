{
  "name": "openai-mock-api",
  "version": "0.4.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openai-mock-api": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "docs:install": "cd docs && npm install"
  },
  "keywords": [
    "openai",
    "mock",
    "api",
    "testing",
    "llm"
  ],
  "author": "patrickjm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/patrickjm/openai-mock-api.git"
  },
  "license": "MIT",
  "description": "A mock OpenAI API server for testing LLM applications",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "commander": "^14.0.0",
    "express": "^5.1.0",
    "fuse.js": "^7.1.0",
    "tiktoken": "^1.0.21",
    "winston": "^3.17.0",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.7",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "axios": "^1.10.0",
    "eslint": "^9.30.0",
    "jest": "^30.0.3",
    "openai": "^5.8.2",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
